  </properties>
  <dependencyManagement>
  	<dependencies>
  		<dependency>
  			<groupId>lsieg.Core</groupId>
  			<artifactId>Core</artifactId>
  			<version>${core.version}</version>
  		</dependency>
  	</dependencies>
